{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate && run-p dev:*",
    "dev:next": "next dev",
    "dev:scss": "typed-scss-modules src --watch --implementation sass --nameFormat none --exportType default --aliasPrefixes.~ src/",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "jest --passWithNoTests",
    "check": "run-s check:*",
    "check:spell": "cspell -c ./cspell.json \"./src/**/*.{ts,tsx}\" --color --no-progress",
    "check:eslint": "eslint src",
    "check:type": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write .",
    "fix:stylelint": "stylelint --fix '**/*.{css,scss}'",
    "typegen:scss": "typed-scss-modules src",
    "generate": "run-p generate:*",
    "generate:api": "rm -rf src/api && npx openapi2aspida -i ../swagger/schema/main.yaml -o src/api",
    "generate:type": "cd ../ && npx openapi-typescript 'swagger/schema/schemas/**/*.yaml' --output front/src/"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@aspida/react-query": "^1.11.0",
    "@types/node": "18.14.4",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "axios": "^0.26.1",
    "date-fns": "^2.29.3",
    "eslint-config-next": "13.2.3",
    "msw": "^1.1.0",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-essentials": "^7.0.0-beta.60",
    "@storybook/addon-interactions": "^7.0.0-beta.60",
    "@storybook/addon-links": "^7.0.0-beta.60",
    "@storybook/blocks": "^7.0.0-alpha.8",
    "@storybook/nextjs": "^7.0.0-beta.60",
    "@storybook/react": "^7.0.0-beta.60",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@swc/core": "^1.3.37",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "chromatic": "^6.17.1",
    "cspell": "^6.27.0",
    "eslint": "^8.35.0",
    "eslint-plugin-import-access": "^1.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "jest": "^29.4.3",
    "msw-auto-mock": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^6.1.1",
    "prettier": "2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "storybook": "^7.0.0-beta.60",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-prettier": "^3.0.0",
    "typed-scss-modules": "^7.0.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
