{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cp .env.sample .env.local && npm run generate && run-p dev:*",
    "dev:next": "next dev",
    "dev:scss": "typed-scss-modules src --watch --implementation sass --nameFormat none --exportType default --aliasPrefixes.~ src/",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:unit": "jest --passWithNoTests",
    "check": "run-s check:*",
    "check:spell": "cspell -c ./cspell.json \"./src/**/*.{ts,tsx}\" --color --no-progress",
    "check:eslint": "eslint src",
    "check:type": "tsc",
    "fix": "run-p fix:*",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write .",
    "fix:stylelint": "stylelint --fix '**/*.{css,scss}'",
    "typegen:scss": "typed-scss-modules src",
    "generate": "run-p generate:*",
    "generate:api": "rm -rf src/api && npx openapi2aspida -i ../swagger/schema/main.yaml -o src/api",
    "generate:type": "cd ../ && npx openapi-typescript@v6.2.0 'swagger/schema/schemas/**/*.yaml' --output frontend/src/",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@aspida/axios": "^1.12.0",
    "@aspida/react-query": "^1.12.0",
    "@radix-ui/react-accessible-icon": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-form": "^0.0.2",
    "@radix-ui/react-popover": "^1.0.5",
    "@radix-ui/react-toast": "^1.1.4",
    "@types/node": "20.3.1",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.6",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "eslint-config-next": "13.4.1",
    "msw": "^1.2.2",
    "next": "13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-loading-icons": "^1.1.0",
    "react-loading-skeleton": "^3.3.1",
    "react-query": "^3.39.3",
    "ts-pattern": "^4.2.3",
    "typescript": "5.0.4",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-a11y": "^7.0.14",
    "@storybook/addon-essentials": "^7.0.14",
    "@storybook/addon-interactions": "^7.0.14",
    "@storybook/addon-links": "^7.0.14",
    "@storybook/addon-mdx-gfm": "^7.0.23",
    "@storybook/blocks": "^7.0.27",
    "@storybook/jest": "^0.1.0",
    "@storybook/nextjs": "^7.0.14",
    "@storybook/react": "^7.0.14",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.2.0",
    "@swc/core": "^1.3.66",
    "@swc/jest": "^0.2.26",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.5",
    "chromatic": "^6.19.9",
    "cspell": "^6.31.1",
    "eslint": "^8.40.0",
    "eslint-plugin-import-access": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "mockdate": "^3.0.5",
    "msw-auto-mock": "^0.13.0",
    "msw-storybook-addon": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^6.2.8",
    "prettier": "2.8.8",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "storybook": "^7.0.14",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-prettier": "^3.0.0",
    "typed-scss-modules": "^7.1.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
